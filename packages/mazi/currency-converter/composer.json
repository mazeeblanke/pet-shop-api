{
    "name": "mazi/currency-converter",
    "description": "A currency rate converter",
    "autoload": {
        "psr-4": {
            "Mazi\\CurrencyConverter\\": "src/"
        }
    },
    "authors": [
        {
            "name": "mazinoukah",
            "email": "ewomaukah@yahoo.com"
        }
    ],
    "require": {
        "darkaonline/l5-swagger": "^8.5",
        "guzzlehttp/guzzle": "^7.5"
    },
    "minimum-stability": "stable",
    "version": "1.0",
    "extra": {
        "laravel": {
            "providers": [
                "Mazi\\CurrencyConverter\\ServiceProvider"
            ]
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0",
        "nunomaduro/phpinsights": "^2.8"
    },
    "config": {
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": true
        }
    }
}
